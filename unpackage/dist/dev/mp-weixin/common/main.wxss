@charset "UTF-8";
/*初始化*/
page,
page,
._p,
._ol,
._ul,
._li,
._dl,
._dt,
._dd,
._blockquote,
._figure,
._fieldset,
._legend,
textarea,
._pre,
._iframe,
._hr,
._h1,
._h2,
._h3,
._h4,
._h5,
._h6 {
	margin: 0;
	padding: 0;
}
._h1,
._h2,
._h3,
._h4,
._h5,
._h6 {
	font-size: 100%;
	font-weight: normal;
}
._ol,
._ul {
	list-style: none;
}
button,
input,
._select,
textarea {
	margin: 0;
	padding: 0;
	border: 0;
	outline: none;
	background: transparent;
}
page {
	box-sizing: border-box;
}
._img,
._embed,
._iframe,
._object,
video {
	height: auto;
	max-width: 100%;
}
audio {
	max-width: 100%;
}
._iframe {
	border: 0;
}
._table {
	border-collapse: collapse;
	border-spacing: 0;
}
._td,
._th {
	padding: 0;
	text-align: left;
}
._a:active,
._a:hover {
	outline: 0;
}
._a,
._a:visited {
	color: #000;
	text-decoration: none;
}
/*图片未加载完时不显示白框*/
._img[src].no_img_opacity,
._img:not([src]).no_img_opacity {
	opacity: 0;
}
page,
page {
	font-family: sans-serif;
	-webkit-touch-callout: none;
	/*禁用系统默认菜单*/
	-webkit-text-size-adjust: none;
	/*禁止移动端 文字缩放*/
	-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
	-webkit-tap-highlight-color: transparent;
	/*重置ios点击灰色背景*/
	-webkit-user-select: none;
	/*禁止选中文字*/
}
.van-pull-refresh {
	overflow: initial !important;
}
